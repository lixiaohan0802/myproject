.main{
  width: 100%;
  height: 100%;
  background-color: #DDDDDD;
  padding-top: 5px;
}
.set{
  width: 90%;
  margin: 10px auto;
  padding-bottom: 10px;
  background-color: white;
  border-radius: 10px;
}
.a{
  width: 90%;
  margin: 0 auto;
  color: grey;
  height: 40px;
  border-bottom:1px solid #BBBBBB;
  line-height: 40px;
  font-size: 16px;
}
.finally{
  height: 200px;
  background-color: #DDDDDD;
}
.set > view image{
  width: 200px;
  height: 200px;
}